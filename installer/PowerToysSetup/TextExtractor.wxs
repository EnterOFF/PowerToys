  <Fragment>
    <DirectoryRef Id="PowerOCRInstallFolder" FileSource="$(var.BinDir)modules\$(var.PowerOCRProjectName)">
    <!-- Generated by generateFileComponents.ps1 -->
    <!--TextExtractorFiles_Component_Def-->
